<p id="notice"><%= notice %></p>

<h1>Exceps</h1>

<table>
  <thead>
    <tr>
      <th>Part</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exceps.each do |excep| %>
      <tr>
        <td><%= excep.part %></td>
        <td><%= link_to 'Show', excep %></td>
        <td><%= link_to 'Edit', edit_excep_path(excep) %></td>
        <td><%= link_to 'Destroy', excep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @exceps %>
<br>

<%= link_to 'New Excep', new_excep_path %>
<br/>
<%= link_to 'Inicio', servicios_index_path %>
